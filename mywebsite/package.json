{
  "name": "mywebsite",
  "version": "1.0.0",
  "description": "个人网站项目 - 集成Figma MCP",
  "scripts": {
    "figma:test": "node scripts/test-figma-connection.js",
    "figma:list": "node scripts/figma-commands.js list-files",
    "figma:file": "node scripts/figma-commands.js get-file",
    "figma:styles": "node scripts/figma-commands.js get-styles",
    "figma:components": "node scripts/figma-commands.js get-components",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "cursor-talk-to-figma-mcp": "^0.2.1"
  },
  "keywords": [
    "figma",
    "mcp",
    "cursor",
    "react",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nextui-org/react": "^2.6.11",
    "axios": "^1.10.0",
    "granim": "^2.0.0",
    "react-router-dom": "^6.30.1",
    "three": "^0.178.0"
  }
}
